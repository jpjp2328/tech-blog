<ul class="post">
    {{#each posts as |post|}}
    <li id={{post.id}}>
        <div>
            <h3>{{post.heading}}</h3>
            <p>Posted by {{post.user.username}} on {{post.created_at}}</p>
        </div>
        <p>{{post.content}}</p>
    </li>
    {{/each}}
</ul>

<script src="/js/homepage.js"></script>